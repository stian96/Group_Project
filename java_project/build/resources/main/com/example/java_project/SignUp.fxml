<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
      prefWidth="600.0"
      xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.java_project.SignUpController" >
    <children>
        <AnchorPane prefHeight="400.0" prefWidth="240.0" style="-fx-background-color: #f1c232;">
            <children>
                <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="20.0" layoutY="88.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@car.png" />
                    </image>
                </ImageView>
                <Label layoutX="55.0" layoutY="227.0" text="ToGoCar">
                    <font>
                        <Font name="Comic Sans MS Bold Italic" size="32.0" />
                    </font></Label>
                <Label layoutX="83.0" layoutY="364.0" text="\@Group5">
                    <font>
                        <Font name="Comic Sans MS Bold" size="10.0" />
                    </font></Label>
            </children></AnchorPane>
        <AnchorPane prefHeight="400.0" prefWidth="360.0" style="-fx-background-color: #9fc5e8;">
            <children>
                <ImageView fitHeight="33.0" fitWidth="51.0" layoutX="164.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@LoginLogo.png" />
                    </image>
                </ImageView>
                <Label layoutX="71.0" layoutY="361.0" text="Already a member?">
                    <font>
                        <Font name="Comic Sans MS Bold" size="11.0" />
                    </font></Label>
                <Label layoutX="57.0" layoutY="92.0" text="Email">
                    <font>
                        <Font name="Comic Sans MS Bold" size="14.0" />
                    </font></Label>
                <Label layoutX="34.0" layoutY="138.0" text="Password">
                    <font>
                        <Font name="Comic Sans MS Bold" size="14.0" />
                    </font></Label>
                <TextField fx:id="tf_userName" layoutX="122.0" layoutY="91.0" promptText="Type your email">
                    <font>
                        <Font name="Comic Sans MS" size="11.0" />
                    </font></TextField>
                <PasswordField fx:id="tf_password" layoutX="122.0" layoutY="137.0" promptText="Type your password">
                    <font>
                        <Font name="Comic Sans MS" size="11.0" />
                    </font></PasswordField>
                <Label layoutX="75.0" layoutY="231.0" text="-------Or use one of these options-------">
                    <font>
                        <Font name="Comic Sans MS Bold" size="11.0" />
                    </font></Label>
                <Button fx:id="button_signUp" layoutX="152.0" layoutY="187.0" mnemonicParsing="false" onAction="#userSignUp" prefHeight="27.0" prefWidth="105.0" style="-fx-background-color: #f1c232; -fx-cursor: hand;" text="Sign Up!">
                    <font>
                        <Font name="Comic Sans MS Bold" size="13.0" />
                    </font></Button>
                <Button fx:id="button_google" layoutX="113.0" layoutY="260.0" mnemonicParsing="false" onAction="#signUpGoogle" style="-fx-cursor: hand; -fx-background-color: #6fc5e9;" text="Sign Up with GOOGLE">
                    <font>
                        <Font name="Comic Sans MS Bold" size="11.0" />
                    </font></Button>
                <Button fx:id="button_facebook" layoutX="113.0" layoutY="295.0" mnemonicParsing="false" onAction="#signUpFacebook" style="-fx-background-color: #6fc5e9; -fx-cursor: hand;" text="Sign Up with Facebook">
                    <font>
                        <Font name="Comic Sans MS Bold" size="11.0" />
                    </font></Button>
                <Button fx:id="button_login" layoutX="188.0" layoutY="357.0" mnemonicParsing="false" onAction="#userLogIn" style="-fx-background-color: transparent; -fx-cursor: hand;" text="Log In!" underline="true">
                    <font>
                        <Font name="Comic Sans MS Bold" size="11.0" />
                    </font></Button>
                <Label fx:id="validEmail" layoutX="200.0" layoutY="65.0" textFill="#ed0f0f">
                    <font>
                        <Font name="Comic Sans MS" size="11.0" />
                    </font>
                </Label>
            </children></AnchorPane>
    </children>
</HBox>